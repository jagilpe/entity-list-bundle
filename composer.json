{
	"name" : "jagilpe/entity-list-bundle",
	"type" : "symfony-bundle",
	"description" : "A Symfony Bundle for the creation and rendering of list of entities",
	"keywords" : [
		"Symfony", "list"
	],
	"license" : "MIT",
	"authors" : [
		{
			"name" : "Javier Gil",
			"email" : "javier@gilpereda.com"
		}
	],
	"require" : {
		"php" : ">=5.6.0",
		"symfony/framework-bundle" : ">=2.3",
		"symfony/twig-bundle" : ">=2.3",
		"sensio/framework-extra-bundle" : "^3.0",
		"doctrine/collections" : ">=1.0",
		"symfony/options-resolver": "^3.2"
	},
	"require-dev" : {
		"symfony/yaml" : ">=2.3",
		"phpunit/phpunit" : "^5.5"
	},
	"autoload" : {
		"psr-4" : {
			"Jagilpe\\EntityListBundle\\" : ""
		},
		"exclude-from-classmap": [
			"/Tests/"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
